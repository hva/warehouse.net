<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>

      <!-- https://oauth.yandex.ru/ http://api.yandex.ru/oauth/doc/dg/tasks/get-oauth-token.xml -->
      <add key="token" value="yandex disk api token"/>

      <!-- backup upload timeout time in "hh:mm[:ss]" format -->
      <add key="uploadTimeout" value="00:03"/>

      <!-- https://code.google.com/archive/p/ncrontab/wikis/CrontabExpression.wiki -->
      <add key="crontab" value="0 21 * * 1-5" />
    </appSettings>
</configuration>